@model IEnumerable<Diplom_Game.Steam_Aksana.Patrubeika.Models.User>

<h2>List of Users</h2>
<table class="table">
    @foreach (var user in Model)
    {
            <tr>
                <td>@user.Email</td>
                <td>
                        <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access permission</a>
                </td>
            </tr>
    }
</table>

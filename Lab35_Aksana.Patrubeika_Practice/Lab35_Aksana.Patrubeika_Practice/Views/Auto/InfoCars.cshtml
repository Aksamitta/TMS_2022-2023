@model List<AutoInfo>

@{
    ViewData["Title"] = "Auto"; 
}

<table class="table"> 
    <tr>
        <th>#</th>
        <th> Model</th>
        <th>Year of Purchase</th>
        <th>Price</th>
        <th>Seller</th>
        <th>Buyer</th>
        <th>Date</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
            <tr>
                <td>@item.Id</td>                
                <td>@item.Model</td>
                <td>@item.Year</td>
                <td>@item.Price</td>
                <td>@item.Seller</td>
                <td>@item.Buyer</td>
                <td>@item.Date</td>
            </tr>
    } 
</table>

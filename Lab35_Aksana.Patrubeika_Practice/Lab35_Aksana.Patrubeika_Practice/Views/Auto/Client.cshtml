@model List<Client> 
@{
    ViewData["Title"] = "All Client"; 
}

<table class="table"> 
    <tr>
        <th>Name</th>
        <th>SecondName</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
                  <tr>
                      <td>
                @Html.DisplayFor(modelItem => item.ClientName)
                      </td>
                      <td>
                @Html.DisplayFor(modelItem => item.ClientSname)
                      </td>
                  </tr>
    } 
 </table>